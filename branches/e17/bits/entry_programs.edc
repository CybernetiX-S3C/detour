
program {
	name: "entry_focus";
	signal: "e,state,focused";
	source: "e";
	action: STATE_SET "focus" 0.0;
	transition: DECELERATE 0.2;
	target: "entry_focus";
}

program {
	name: "entry_unfocus";
	signal: "e,state,unfocused";
	source: "e";
	action: STATE_SET "default" 0.0;
	transition: DECELERATE 0.2;
	target: "entry_focus";
}
